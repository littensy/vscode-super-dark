

{
    "name": "Super Dark",
    "type": "Dark",
    "colors": {
        "editor.background": "#141414",
        "editor.foreground": "#C3CCDB",
        "editorIndentGuide.background": "#292929",
        "editorRuler.foreground": "#292929",

        "activityBar.background": "#0C0C0C",
        "activityBar.foreground": "#C8C8C8",
        "activityBarBadge.background": "#F04747",

        "titleBar.activeBackground": "#0C0C0C",
        "titleBar.activeForeground": "#B6B6B6",
        "titleBar.inactiveBackground": "#0C0C0C",
        "titleBar.inactiveForeground": "#B6B6B6",

        "sideBar.background": "#111111",
        "sideBar.foreground": "#BDBDBD",
        "sideBarTitle.foreground": "#BDBDBD",
        "sideBarSectionHeader.background": "#0C0C0C",
        "sideBarSectionHeader.foreground": "#B6B6B6",

        "editor.selectionBackground": "#282828",

        "editorLineNumber.foreground": "#585858",
        "editorLineNumber.activeForeground": "#585858",
        "editorGroup.background": "#0C0C0C",
        "editorGroupHeader.noTabsBackground": "#0C0C0C",
        "editorGroupHeader.tabsBackground": "#0C0C0C",
        "editorCursor.foreground": "#FFCC00",

        "tab.activeBackground": "#141414",
        "tab.inactiveBackground": "#111111",

        "dropdown.background": "#111111",
        "dropdown.listBackground": "#111111",

        "list.activeSelectionBackground": "#1A1A1A",
        "list.focusBackground": "#1A1A1A",
        "list.hoverBackground": "#111111",
        "list.inactiveSelectionBackground": "#111111",
        "list.inactiveFocusBackground": "#111111",

        "statusBar.background": "#242424",
        "statusBar.debuggingBackground": "#F04747",
        "statusBar.noFolderBackground": "#0C0C0C",

        "input.background": "#242424"
    },
    "tokenColors": [
        {
            "name": "Default Text",
            "scope": [
                "support.variable",
                "variable.other.object"
            ],
            "settings": {
                "foreground": "#C3CCDB"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword.control",
                "keyword.local",
                "storage.modifier",

                "meta.method.return-type",
                "storage.type.primitive",
                "storage.type.built-in",
                "storage.type.struct",

                "constant.language",
                "storage.type.namespace.directive",
                "keyword.other.using.directive",
                "entity.name.namespace.cpp"
            ],
            "settings": {
                "foreground": "#D680EF"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#424242",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#A6D585"
            }
        },
        {
            "name": "String Escapes",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#E5C07B"
            }
        },
        {
            "name": "Other Variables & Types",
            "scope": [
                "variable",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#E06C75"
            }
        },
        {
            "name": "Operators & Symbols",
            "scope": [
                "keyword.operator",
                "source.lua",

                "punctuation.section",
                "punctuation.definition",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.bracket"
            ],
            "settings": {
                "foreground": "#56B6C2"
            }
        },
        {
            "name": "Special Keywords",
            "scope": [
                "keyword.control.directive",
                "keyword.other.import"
            ],
            "settings": {
                "foreground": "#56B6C2"
            }
        },
        {
            "name": "Namespace",
            "scope": [
                "entity.name.scope-resolution"
            ],
            "settings": {
                "foreground": "#C3CCDB"
            }
        },
        {
            "name": "Class",
            "scope": [
                "support.class",
                "storage.type",

                "entity.name.type.class",
                "entity.name.class",

                "entity.name.type.struct"
            ],
            "settings": {
                "foreground": "#E5C07B"
            }
        },
        {
            "name": "Class Members",
            "scope": [
                "variable.other.lua",
                "variable.other.object.property"
            ],
            "settings": {
                "foreground": "#C3CCDB"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant.language.java",
                "constant.language.lua",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#E4A06D"
            }
        },
        {
            "name": "Function Calls & Constructors",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.body.function",
                "variable.other.global"
            ],
            "settings": {
                "foreground": "#61B8FF"
            }
        },
        {
            "name": "Function Parameters",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#FF5370",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Immutable Variables & Types",
            "scope": [
                "variable.language.self",
                "variable.language.this"
            ],
            "settings": {
                "foreground": "#FF5370",
                "fontStyle": "italic"
            }
        }
    ]
}